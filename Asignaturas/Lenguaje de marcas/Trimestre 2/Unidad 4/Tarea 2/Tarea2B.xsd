            <xs:sequence>
                <xs:element name="duracion" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element name="mome" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element name="ciclo" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
                
                <xs:element name="contidos">
                    <xs:complexType>
                        <sequence>
